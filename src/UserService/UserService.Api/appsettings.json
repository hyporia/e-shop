{
  "ConnectionStrings": {
    "userDb": null
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "RabbitMQ.Client": "Information",
      "OpenIddict": "Trace"
    }
  },
  "AllowedHosts": "*",
  "IdentityOptions": {
    "Password": {
      "RequiredLength": 6,
      "RequireDigit": true,
      "RequireLowercase": true,
      "RequireNonAlphanumeric": true,
      "RequireUppercase": true
    }
  }
}
